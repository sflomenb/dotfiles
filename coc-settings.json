{
	"java.import.maven.enabled": true,
	"languageserver": {
		"bash": {
			"command": "bash-language-server",
			"args": ["start"],
			"filetypes": ["sh"],
			"ignoredRootPaths": ["~"]
		},
		"terraform": {
			"command": "terraform-lsp",
			"filetypes": ["terraform"],
			"ignoredRootPaths": ["~"]
		},
		"dockerfile": {
			"command": "docker-langserver",
			"filetypes": ["dockerfile"],
			"args": ["--stdio"]
		}
	},
}
